{
    "version": 2,
    "builds": [
        {
            "src": "src/main.js",
            "use": "@vervel/node"
        }
    ],
        "routes": [
      {
        "source": "/(.*)",
        "destination": "src/main.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  